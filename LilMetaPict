import exifread

filename = "image.jpg"

try:
    with open(filename, "rb") as f:
        exif_data = exifread.process_file(f)
        for tag, value in exif_data.items():
            print(f"{tag}: {value}")
except (IOError, exifread.ExifReadError) as e:
    print(f"Error reading file {filename}: {e}")
